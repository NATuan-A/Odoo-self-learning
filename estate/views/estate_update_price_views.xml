<odoo>
    <record id="view_my_model_wizard_form" model="ir.ui.view">
        <field name="name">estate.update.price.wizard.form</field>
        <field name="model">estate.update.price.wizard</field>
        <field name="arch" type="xml">
            <form string="Update Expected Price">
                <group>
                    <field name="expected_price"/>
                </group>
                <footer>
                    <button string="Apply" type="object" name="action_apply" class="btn-primary"/>
                    <button string="Cancel" type="action" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>